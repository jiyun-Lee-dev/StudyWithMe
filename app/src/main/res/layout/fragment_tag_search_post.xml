             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".Board.TagSearchPost">

    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
